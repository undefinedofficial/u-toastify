{
  "name": "u-toastify",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/u-toastify.cjs",
  "module": "./dist/u-toastify.js",
  "types": "./dist/u-toastify.d.ts",
  "exports": {
    ".": {
      "import": "./dist/u-toastify.js",
